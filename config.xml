<?xml version='1.0' encoding='utf-8'?>
<widget id="com.puzzlemasterdev.puzzlemaster" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>Puzzle Master</name>
    <description>Challenge your mind with sliding number puzzles</description>
    <author email="srinivasgundale@gmail.com" href="https://srinivas-gundali.vercel.app/">Srinivas</author>
    
    <content src="index.html" />
    <access origin="*" />
    
    <preference name="DisallowOverscroll" value="true" />
    <preference name="android-minSdkVersion" value="22" />
    <preference name="android-targetSdkVersion" value="33" />
    
    <platform name="android">
        <allow-intent href="market:*" />
